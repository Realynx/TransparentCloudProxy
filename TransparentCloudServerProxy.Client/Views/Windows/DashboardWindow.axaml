<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="TransparentCloudServerProxy.Client.DashboardWindow"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="DashboardWindow"
        WindowStartupLocation="CenterScreen"
        Width="1900" Height="1280"
        TransparencyLevelHint="AcrylicBlur"
        ExtendClientAreaToDecorationsHint="True"
        ExtendClientAreaChromeHints="NoChrome"
        Background="Transparent"
		x:DataType="vm:DashboardWindowViewModel"
		xmlns:vm="clr-namespace:TransparentCloudServerProxy.Client.ViewModels.Windows">


	<Border Background="#333"
            CornerRadius="16"
            Padding="20"
            PointerPressed="DragWindow">
		<ContentControl Content="{Binding CurrentPage}" />
	</Border>
</Window>
